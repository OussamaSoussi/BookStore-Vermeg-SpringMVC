package org.vermeg.polytech.controllers;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.vermeg.polytech.model.Book;
import com.vermeg.polyteh.Service.IService;

@RestController
@RequestMapping("/Book")
public class BookController {

	@Autowired
	private IService<Book> productService;
	public BookController() {}
	public BookController(IService<Book> productService) {
		this.productService = productService;
	}
}
